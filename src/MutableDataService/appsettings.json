{
  "urls": "https://*:65533",
  "MutableDataService": {
    "Addresses": {
      "DatabaseService": "https://localhost:65535",
      "OpenApiPath": "/openapi/v1.json"
    }
  },
  "JsonSettings": {
  "WriteIndented": true,
  "ReferenceHandler": "Ignore"
  },
  "ResilienceSettings": {
    "MaxRetryCount": 5,
    "MaxDelay": "00:10:00",
    "CircuitBreakerThreshold": 0.2,
    "RequestsPerSecond": 15
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
